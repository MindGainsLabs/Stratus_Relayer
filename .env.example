# Stratus_Relayer Environment Configuration

# Server Configuration
PORT=80
NODE_ENV=development

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/stratus_relayer

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-here

# Discord Bot Configuration (for message collection)
DISCORD_TOKEN=your-discord-bot-token-here
CHANNEL_ID_1=your-discord-channel-id-1
CHANNEL_ID_2=your-discord-channel-id-2
CHANNEL_ID_3=your-discord-channel-id-3

# Cron Job Configuration
CRON_SCHEDULE=*/2 * * * * *  # Every 2 seconds
CRON_HOURS=6  # Look back 6 hours

# Rugcheck API Configuration (for risk analysis)
RUGCHECK_API_URL=https://api.rugcheck.xyz

# WebSocket Configuration
WS_AUTO_BROADCAST_INTERVAL=30000  # 30 seconds
WS_MAX_CLIENTS=1000

# Logging
LOG_LEVEL=info

# CORS Configuration
CORS_ORIGINS=https://smarteye.one,http://localhost:3000,http://localhost:8080
